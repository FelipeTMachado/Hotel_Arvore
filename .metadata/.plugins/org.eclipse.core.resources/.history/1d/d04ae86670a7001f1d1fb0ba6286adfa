package model;

public class Hotel {
	// ATRIBUTOS
	private int id;
	private String nome;
	
	
	
	// CONTRUTOR
	public Hotel() {
		
	}

	

	// GETTERS AND SETTERS
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
}
